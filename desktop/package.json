{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "scripts": {
    "start": "electron dist/main.js",
    "start:hot": "webpack-dev-server --config webpack.electron-front.ts",
    "start:web": "parcel src/index.html",
    "build:web": "parcel build index.html",
    "build:electron": "webpack-cli --config webpack.electron.ts --progress",
    "prebuild:web": "rm -rf dist",
    "prebuild:electron": "rm -rf dist",
    "prestart": "yarn run build:electron"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/react-router-dom": "^4.2.7",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "electron": "^2.0.2",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.3",
    "standard": "^11.0.1",
    "standard-loader": "^6.0.1",
    "style-loader": "^0.21.0",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "@types/html-webpack-plugin": "^2.30.3",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "@types/webpack": "^4.4.0",
    "bootstrap": "^4.1.1",
    "parcel-bundler": "^1.8.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.3.1",
    "ts-loader": "^4.4.1",
    "ts-node": "^6.1.1",
    "typescript": "^2.9.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4",
    "yarn": "^1.7.0"
  }
}
