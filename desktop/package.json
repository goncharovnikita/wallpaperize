{
  "name": "wallpaperize-desktop",
  "version": "1.0.2-beta",
  "description": "A wallpaperize desktop application",
  "repository": {
    "url": "https://github.com/goncharovnikita/wallpaperize"
  },
  "scripts": {
    "compile": "electron-webpack",
    "start": "electron-webpack dev",
    "start:hot": "webpack-dev-server --config webpack.electron-front.ts",
    "build:prod": "webpack-cli --config webpack.electron-prod.ts --progress",
    "build:electron": "webpack-cli --config webpack.electron.ts --progress",
    "pack": "electron-builder --dir",
    "dist": "electron-builder && build --publish always",
    "dist:all": "electron-builder -m -l --publish always",
    "precompile": "rimraf dist",
    "dist:simple": "electron-builder",
    "predist": "yarn compile",
    "predist:all": "yarn compile",
    "predist:simple": "yarn compile",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "Wallpaperize"
  ],
  "author": "Goncharov Nikita",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.1",
    "electron-log": "^2.2.15",
    "fix-path": "^2.1.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "source-map-support": "^0.5.5",
    "yarn": "^1.7.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0-beta.51",
    "@types/html-webpack-plugin": "^2.30.3",
    "@types/react": "^16.3.18",
    "@types/react-dom": "^16.0.6",
    "@types/react-router": "^4.0.26",
    "@types/react-router-dom": "^4.2.7",
    "@types/webpack": "^4.4.0",
    "electron": "^2.0.2",
    "electron-builder": "20.14.7",
    "electron-updater": "^2.21.10",
    "electron-webpack": "^2.0.1",
    "electron-webpack-ts": "^2.0.2",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.3",
    "typescript": "^2.9.2",
    "webpack": "^4.8.1"
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  },
  "electronWebpack": {
    "renderer": {
      "webpackConfig": "webpack-alias.js"
    }
  },
  "build": {
    "appId": "org.electron.wallpaperize"
  }
}
