{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": false,
        "member-ordering": false,
        "trailing-comma": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "only-arrow-functions": false,
        "member-access": false,
        "no-console": false,
        "interface-name": false,
        "arrow-parens": false,
        "variable-name": false,
        "no-string-literal": false
    },
    "rulesDirectory": []
}